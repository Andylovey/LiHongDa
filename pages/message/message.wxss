.search-contain{background: #fff; width: 100%; height: 68rpx; padding: 14rpx; border-bottom: 1rpx solid #ccc; position: fixed; top: 0; z-index: 999;}
.search-contain .search-content{width: 570rpx; height: 68rpx; padding:0 10rpx; margin-right: 14rpx; border: 1rpx solid #7a7a7a; font-size: 32rpx; line-height: 32rpx; text-align: left; display: inline-block;}
.search-contain .btn-search{width: 110rpx; height: 68rpx; background: #169bd5; padding: 18rpx 0; font-size: 32rpx; line-height: 32rpx; color: #fff; display: inline-block;}

.message-contain{background: #fff; margin-top: 101rpx;}
.message-contain .message-item{padding: 14rpx; border-bottom: 1px solid #cccccc;}
.message-contain .message-item .message-title{font-size: 32rpx; line-height: 40rpx; color: #000;}
.message-contain .message-item .message-title .title{display: inline-block;}
.message-contain .message-item .message-title .title .type{color: #ff6600; display: inline-block;}
.message-contain .message-item .message-title .title .state {color: #ff6600; display: inline-block;}
.message-contain .message-item .message-title .btn-detail{float: right; position: relative; top: 18rpx; right: 10rpx; display: inline-block;}
.message-contain .message-item .message-title .btn-detail .message-arrow{width: 16rpx; height: 26rpx;}
.message-contain .message-item .message-content{font-size: 32rpx; line-height: 32rpx;}
.message-contain .message-item .message-content .message-content-item{color: #666666;}