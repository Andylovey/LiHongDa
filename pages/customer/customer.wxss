.search-contain{background: #fff; width: 100%; height: 68rpx; padding: 14rpx; border-bottom: 1rpx solid #ccc; position: fixed; top: 0; z-index: 999;}
.search-contain .search-content{width: 570rpx; height: 68rpx; padding:0 10rpx; margin-right: 14rpx; border: 1rpx solid #7a7a7a; font-size: 32rpx; line-height: 32rpx; text-align: left; display: inline-block;}
.search-contain .btn-search{width: 110rpx; height: 68rpx; background: #169bd5; padding: 18rpx 0; font-size: 32rpx; line-height: 32rpx; color: #fff; display: inline-block;}

.customer-contain{background: #fff; margin-top: 101rpx;}
.customer-contain .customer-item{padding: 14rpx 20rpx; border-bottom: 1px solid #cccccc;}
.customer-contain .customer-item .customer-title{font-size: 32rpx; line-height: 40rpx; color: #000;}
.customer-contain .customer-item .customer-title .title{display: inline-block;}
.customer-contain .customer-item .customer-title .btn-detail{float: right; position: relative; top: 28rpx; right: 10rpx; display: inline-block;}
.customer-contain .customer-item .customer-title .btn-detail .customer-arrow{width: 16rpx; height: 26rpx;}
.customer-contain .customer-item .customer-content{width: 100%; font-size: 26rpx; line-height: 32rpx; display: inline-block;}
.customer-contain .customer-item .customer-content .customer-content-item{width: 30%; color: #666666; display: inline-block;}
.customer-contain .customer-item .customer-content .customer-content-item:nth-child(2){width: 18%;}
.customer-contain .customer-item .customer-content .customer-content-item:nth-child(3){width: 52%;}