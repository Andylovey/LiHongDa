page{background: #e0e0e0;}
navigator{display: inline-block;}

.search-contain{background: #fff; width: 100%; height: 68rpx; padding: 14rpx; border-bottom: 1rpx solid #ccc; position: fixed; top: 0; z-index: 999; display: inline-block;}
.search-contain .search-filter{float: left; width: 140rpx; height: 68rpx; border: 1rpx solid #7a7a7a; display: inline-block;}
.search-contain .search-filter .text{padding:0 10rpx; font-size: 30rpx; line-height: 68rpx;}
.search-contain .search-content{float: left; width: 376rpx; height: 68rpx; padding:0 10rpx; border: 1rpx solid #7a7a7a; border-left: 0; font-size: 30rpx; line-height: 30rpx; text-align: left; display: inline-block;}
.search-contain .search-scan{float: left; width: 68rpx; height: 68rpx; margin-left: 6rpx; margin-right: 6rpx; display: inline-block;}
.search-contain .btn-search{float: left; width: 110rpx; background: #169bd5; padding: 18rpx 0; font-size: 30rpx; line-height: 30rpx; color: #fff; display: inline-block;}

.tast-contain{padding: 115rpx 14rpx 14rpx;}
.tast-contain .tast-item{background: #fff; margin-bottom: 16rpx; border-radius: 14rpx;}
.tast-contain .tast-item:last-child{border-bottom: 0; margin-bottom: 0;}
.tast-contain .tast-item .tast-title{background: #ff6600; padding: 22rpx 16rpx 18rpx; border-top-left-radius: 16rpx; border-top-right-radius: 16rpx; font-size: 32rpx; line-height: 32rpx; color: #fff;}
.tast-contain .tast-item .tast-title .title{display: inline-block;}
.tast-contain .tast-item .tast-title .btn-detail{float: right; display: inline-block;}
.tast-contain .tast-item .tast-title .btn-detail .text{position: relative; bottom: 2rpx; right: 10rpx; display: inline-block;}
.tast-contain .tast-item .tast-title .btn-detail .tast-arrow{width: 16rpx; height: 26rpx;}
.tast-contain .tast-item .tast-content{padding: 10rpx 14rpx; border-bottom: 1px solid #cccccc;}
.tast-contain .tast-item .tast-content .tast-content-item{margin-bottom: 2rpx; font-size: 30rpx; color: #666666;}
.tast-contain .tast-item .tast-content .tast-content-item .tast-number{width: 100%; font-size: 26rpx; display: inline-block;}
.tast-contain .tast-item .tast-content .tast-content-item .tast-number .tast-number-item{width: 30%; color: #ff6600; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; display: inline-block;}
.tast-contain .tast-item .tast-content .tast-content-item .tast-number .tast-number-item:first-child{width: 40%;}
.tast-contain .tast-item .tast-content .tast-content-item-right{float: right; height: 0; font-size: 32rpx; color: #ff6600; position: relative; bottom: 190rpx; right: 30rpx; display: inline-block; z-index: 998;}
.tast-contain .tast-item .tast-content .text-red{color: #ff0000;}
.tast-contain .tast-item .tast-content .text-blue{color: #006699;}
.tast-contain .tast-item .tast-content .text-green{color: #006600;}
.tast-contain .tast-item .tast-footer{padding: 14rpx; text-align: right;}
.tast-contain .tast-item .tast-footer .btn-tast{background: #006699; width: 100rpx; height: 28rpx; padding: 16rpx 0; margin-left: 10rpx; border-radius: 10rpx; font-size: 28rpx; line-height: 28rpx; color: #fff; text-align: center; display: inline-block;}

.no-data{background: #e0e0e0; border-bottom: 1rpx dashed #9b9b9b;}
.no-data .text{background: #e0e0e0; color: #9b9b9b;}