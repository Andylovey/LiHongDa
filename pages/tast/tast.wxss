.tast-contain{padding: 30rpx; display: inline-block;}
.tast-contain .tast-item{float: left; width: 33.3%; margin-bottom: 12rpx; text-align: center; display: inline-block;}
.tast-contain .tast-item .content{width: 200rpx; height: 200rpx; border: 4rpx solid #ff6600; border-radius: 12rpx; display: inline-block;}
.tast-contain .tast-item .content .parent{width: 100%; height: 100%; display: table; margin: 0 auto;}
.tast-contain .tast-item .content .parent .chlid{display: table-cell; vertical-align: middle; color: #ff6600; font-size: 34rpx; line-height: 34rpx;}